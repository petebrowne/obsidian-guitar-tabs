    "@vitejs/plugin-react": "^5.1.1",
    "esbuild": "0.17.3",
    "typescript": "5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "builtin-modules": "3.3.0",
    "es-toolkit": "^1.42.0",
    "obsidian": "latest",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tslib": "^2.8.1"
